#include <stdio.h>

int main() {
    float num;
    int count_positive = 0;
    float sum_positive = 0;

    for (int i = 0; i < 6; i++) {
        scanf("%f", &num);
        if (num > 0) {
            count_positive++;
            sum_positive += num;
        }
    }

    printf("%d valores positivos\n", count_positive);
    printf("%.1f\n", sum_positive / count_positive);

    return 0;
}
