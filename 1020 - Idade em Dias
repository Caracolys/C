#include <stdio.h>

int main() {
    int ageInDays, years, months, days;

    scanf("%d", &ageInDays);

    years = ageInDays / 365;
    ageInDays %= 365;

    months = ageInDays / 30;
    ageInDays %= 30;

    days = ageInDays;

    printf("%d ano(s)\n%d mes(es)\n%d dia(s)\n", years, months, days);

    return 0;
}
